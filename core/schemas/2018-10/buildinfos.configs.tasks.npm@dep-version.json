{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "./buildinfos.configs.tasks.npm@dep-version.json",
    "type": "object",
    "properties": {
        "depNamePatterns": {
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            ]
        },
        "distTag": {
            "type": "string",
            "examples": [
                "latest",
                "next",
                "@github-release:<org>:<repo>"
            ]
        }
    }
}